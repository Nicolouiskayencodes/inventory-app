<li>
  <% if (image!==null && image!=='') { %>
    <img src=<%= image %>/>
  <%}%>
  <h2><%= title%></h2>
  <% if (artist) { %>
    <p>Artist: <a href='/artist/<%= artistid%>'><%= artist %></a></p>
  <% } %>
  <% if (created!==null) { %>
    <p>Created on <%= created.toDateString() %></p>
  <% } %>
  <% if (price !==null) { %>
    <p>Price: $<%= price %></p>
  <%}%>
  <% if (type) { %>
    <p>Medium: <a href="/type/<%= typeid%>"><%= type %></a></p>
  <% }%>
  <button><a href='/update/<%= id%>'>Update</a></button>
</li>