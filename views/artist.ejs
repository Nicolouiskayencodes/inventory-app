<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/styles.css">
  <title><%=artList[0].artist_name%></title>
</head>
<body>
  <nav><button><a href="/">Home</a></button></nav>
  <h1><%=artList[0].artist_name%></h1>
  <button class="delete"><a href="/deleteartist/<%= id %>">Delete <%=artist_name%></a></button>
  <% if (artList) {%>
  <ul>
    <% artList.forEach(piece => { %>
      <%- include('piece', {title: piece.art_name, created: piece.created, price: piece.price, image: piece.image, artist: null, type: piece.type_name, artistid: piece.artistid, typeid: piece.typeid, id: piece.art_id})%>
      
    <% }); %>
  </ul>
  <% } %>
</body>
</html>